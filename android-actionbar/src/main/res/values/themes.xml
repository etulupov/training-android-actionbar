<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    </style>


    <style name="AppTheme.Overlay">
        <item name="actionBarStyle">@style/Widget.ActionBar.Transparent</item>
        <item name="windowActionBarOverlay">true</item>
    </style>

    <style name="AppTheme.CustomActionBar">
        <item name="actionBarStyle">@style/Widget.ActionBar.Custom</item>
        <item name="actionBarSplitStyle">@style/Widget.ActionBar.Custom</item>
        <item name="actionBarTabStyle">@style/Widget.ActionBar.TabView</item>
    </style>

</resources>
